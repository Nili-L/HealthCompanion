{
      "name": "health-companion",
      "version": "1.0.0",
      "private": true,
      "description": "HIPAA-compliant healthcare companion app for patients and providers",
      "author": "Anny Levine",
      "dependencies": {
            "@capacitor/android": "^7.4.3",
            "@capacitor/cli": "^7.4.3",
            "@capacitor/core": "^7.4.3",
            "@hookform/resolvers": "^3.9.1",
            "@jsr/supabase__supabase-js": "^2.49.8",
            "@radix-ui/react-accordion": "^1.2.3",
            "@radix-ui/react-alert-dialog": "^1.1.6",
            "@radix-ui/react-aspect-ratio": "^1.1.2",
            "@radix-ui/react-avatar": "^1.1.3",
            "@radix-ui/react-checkbox": "^1.1.4",
            "@radix-ui/react-collapsible": "^1.1.3",
            "@radix-ui/react-context-menu": "^2.2.6",
            "@radix-ui/react-dialog": "^1.1.6",
            "@radix-ui/react-dropdown-menu": "^2.1.6",
            "@radix-ui/react-hover-card": "^1.1.6",
            "@radix-ui/react-label": "^2.1.2",
            "@radix-ui/react-menubar": "^1.1.6",
            "@radix-ui/react-navigation-menu": "^1.2.5",
            "@radix-ui/react-popover": "^1.1.6",
            "@radix-ui/react-progress": "^1.1.2",
            "@radix-ui/react-radio-group": "^1.2.3",
            "@radix-ui/react-scroll-area": "^1.2.3",
            "@radix-ui/react-select": "^2.1.6",
            "@radix-ui/react-separator": "^1.1.2",
            "@radix-ui/react-slider": "^1.2.3",
            "@radix-ui/react-slot": "^1.1.2",
            "@radix-ui/react-switch": "^1.1.3",
            "@radix-ui/react-tabs": "^1.1.3",
            "@radix-ui/react-toggle": "^1.1.2",
            "@radix-ui/react-toggle-group": "^1.1.2",
            "@radix-ui/react-tooltip": "^1.1.8",
            "@supabase/supabase-js": "*",
            "astral-regex": "^2.0.0",
            "class-variance-authority": "^0.7.1",
            "clsx": "*",
            "cmdk": "^1.1.1",
            "dompurify": "^3.2.3",
            "embla-carousel-react": "^8.6.0",
            "hono": "*",
            "input-otp": "^1.4.2",
            "lucide-react": "^0.487.0",
            "next-themes": "^0.4.6",
            "react": "^18.3.1",
            "react-day-picker": "^8.10.1",
            "react-dom": "^18.3.1",
            "react-hook-form": "^7.55.0",
            "react-resizable-panels": "^2.1.7",
            "recharts": "^2.15.2",
            "sonner": "^2.0.3",
            "tailwind-merge": "*",
            "vaul": "^1.1.2",
            "zod": "^3.24.1"
      },
      "devDependencies": {
            "@eslint/js": "^9.17.0",
            "@testing-library/jest-dom": "^6.6.3",
            "@testing-library/react": "^16.1.0",
            "@testing-library/user-event": "^14.5.2",
            "@types/node": "^20.10.0",
            "@typescript-eslint/eslint-plugin": "^8.21.0",
            "@typescript-eslint/parser": "^8.21.0",
            "@vitejs/plugin-react-swc": "^3.10.2",
            "@vitest/ui": "^2.1.8",
            "concurrently": "^9.2.1",
            "cross-env": "^10.1.0",
            "electron": "^38.2.0",
            "electron-builder": "^26.0.12",
            "eslint": "^9.17.0",
            "eslint-plugin-react": "^7.37.2",
            "eslint-plugin-react-hooks": "^5.1.0",
            "eslint-plugin-react-refresh": "^0.4.23",
            "eslint-plugin-security": "^3.0.1",
            "globals": "^15.14.0",
            "jsdom": "^25.0.1",
            "typescript-eslint": "^8.21.0",
            "vite": "^6.3.6",
            "vitest": "^2.1.8",
            "wait-on": "^9.0.1"
      },
      "scripts": {
            "dev": "vite",
            "build": "vite build",
            "type-check": "tsc --noEmit",
            "lint": "eslint . --max-warnings 0",
            "lint:fix": "eslint . --fix",
            "test": "vitest",
            "test:ui": "vitest --ui",
            "test:coverage": "vitest --coverage",
            "prepare": "husky install",
            "electron": "cross-env NODE_ENV=development electron .",
            "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:3000 && npm run electron\"",
            "electron:build": "npm run build && electron-builder",
            "electron:build:win": "npm run build && electron-builder --win",
            "electron:build:mac": "npm run build && electron-builder --mac",
            "electron:build:linux": "npm run build && electron-builder --linux",
            "android:sync": "npm run build && npx cap sync android",
            "android:open": "npx cap open android",
            "android:build": "npm run build && npx cap sync android && cd android && .\\gradlew assembleRelease"
      },
      "main": "electron/main.js",
      "build": {
            "appId": "com.healthcompanion.app",
            "productName": "Health Companion",
            "directories": {
                  "output": "dist-electron"
            },
            "files": [
                  "dist/**/*",
                  "electron/**/*"
            ],
            "win": {
                  "target": [
                        "nsis"
                  ]
            },
            "mac": {
                  "target": [
                        "dmg"
                  ],
                  "category": "public.app-category.healthcare-fitness"
            },
            "linux": {
                  "target": [
                        "AppImage"
                  ],
                  "category": "Health"
            },
            "nsis": {
                  "oneClick": false,
                  "allowToChangeInstallationDirectory": true,
                  "createDesktopShortcut": true,
                  "createStartMenuShortcut": true
            }
      }
}
